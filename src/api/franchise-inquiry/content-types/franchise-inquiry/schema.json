{
  "kind": "collectionType",
  "collectionName": "franchise_inquiries",
  "info": {
    "singularName": "franchise-inquiry",
    "pluralName": "franchise-inquiries",
    "displayName": "Franchise Inquiry"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "fullName": {
      "type": "string",
      "required": true
    },
    "country": {
      "type": "enumeration",
      "enum": [
        "Saudi Arabia",
        "UAE",
        "Qatar",
        "Kuwait",
        "Bahrain",
        "Oman",
        "Lebanon",
        "Jordan",
        "Egypt",
        "Turkey"
      ],
      "required": true
    },
    "telephone1": {
      "type": "string",
      "required": true
    },
    "telephone2": {
      "type": "string"
    },
    "fax": {
      "type": "string"
    },
    "email": {
      "type": "email",
      "required": true
    },
    "companyName": {
      "type": "text",
      "required": true
    },
    "industry": {
      "type": "text",
      "required": true
    },
    "address": {
      "type": "text",
      "required": true
    },
    "interestCountry": {
      "type": "enumeration",
      "enum": [
        "Saudi Arabia",
        "UAE",
        "Qatar",
        "Kuwait",
        "Bahrain",
        "Oman",
        "Lebanon",
        "Jordan",
        "Egypt",
        "Turkey"
      ],
      "required": true
    },
    "interestState": {
      "type": "string",
      "required": true
    },
    "investment": {
      "type": "enumeration",
      "enum": [
        "US 5,000,001 - US 10,000,000",
        "US 10,000,001 - US 20,000,000",
        "Above US 20,000,000"
      ],
      "default": "US 5,000,001 - US 10,000,000"
    },
    "franchiseTimeline": {
      "type": "enumeration",
      "enum": [
        "Within 3 Months",
        "Within 6 Months",
        "Within 12 Months"
      ],
      "default": "Within 3 Months"
    },
    "havePremises": {
      "type": "enumeration",
      "enum": [
        "No",
        "Yes"
      ],
      "default": "No"
    },
    "haveExperience": {
      "type": "enumeration",
      "enum": [
        "No",
        "Yes"
      ],
      "default": "No"
    },
    "discoverSource": {
      "type": "enumeration",
      "enum": [
        "Email or Direct Marketing",
        "Advertisement",
        "Website",
        "Others"
      ],
      "default": "Email or Direct Marketing"
    },
    "remarks": {
      "type": "text",
      "required": true
    }
  }
}
